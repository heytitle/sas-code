DATA IQ;
	SET IQ;
	IF ( IQ < 100 ) THEN
		BELOW_100 = 1;
	ELSE
		BELOW_100 = 0;
		
	DEPART=GROUP;
RUN;

PROC SQL ;
	CREATE TABLE TIES AS
	SELECT DEPART, IQ, COUNT(IQ) AS TIES FROM IQ GROUP BY DEPART, IQ;
RUN;

PROC PRINT DATA=TIES;
	VAR DEPART IQ TIES;
RUN;



PROC UNIVARIATE data=IQ normal;
	VAR IQ;
	BY GROUP;
	qqplot / normal(mu=est sigma=est);
RUN;

PROC GLM DATA=IQ;
	CLASS GROUP;
    MODEL IQ = GROUP;
	MEANS GROUP / HOVTEST=BARTLETT;
RUN;

PROC GLM DATA=IQ;
	CLASS GROUP;
    MODEL IQ = GROUP;
	MEANS GROUP / HOVTEST=BF;
RUN;

PROC GLM DATA=IQ;
	CLASS GROUP;
    MODEL IQ = GROUP;
	MEANS GROUP / HOVTEST=LEVENE;
